{
  "appId": "com.nvisionfilms.turnkey",
  "appName": "TurnKey",
  "webDir": "dist",
  "server": {
    "url": "https://helpmefilm.com",
    "androidScheme": "https",
    "allowNavigation": ["*.railway.app", "backend-backend-c520.up.railway.app", "helpmefilm.com", "*.helpmefilm.com"],
    "cleartext": false
  },
  "android": {
    "allowMixedContent": true
  },
  "plugins": {
    "CapacitorHttp": {
      "enabled": true
    },
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#06101E",
      "showSpinner": false
    },
    "StatusBar": {
      "style": "Dark",
      "backgroundColor": "#06101E"
    }
  }
}
